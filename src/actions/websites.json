{
  "list": [
    {
      "addresses": ["arstechnica.com"],
      "selectors": [".article", "li > article"]
    },
    {
      "addresses": ["digg.com"],
      "selectors": ["article"]
    },
    {
      "addresses": ["facebook.com", "m.facebook.com", "www.facebook.com"],
      "selectors": ["[role=article]", "article"]
    },
    {
      "addresses": ["instagram.com", "www.instagram.com"],
      "hideInsteadOfRemove": true,
      "selectors": ["article"]
    },
    {
      "addresses": ["news.ycombinator.com"],
      "selectors": ["tr.athing"]
    },
    {
      "addresses": ["old.reddit.com", "reddit.com", "www.reddit.com"],
      "selectors": [
        "[class*=scrollerItem]",
        "[href^=\\/search]",
        "body.listing-page div[data-url]",
        "body:not(.comments-page) article"
      ]
    },
    {
      "addresses": ["tildes.net"],
      "selectors": ["ol.topic-listing > li"]
    },
    {
      "addresses": ["twitter.com"],
      "hideInsteadOfRemove": true,
      "selectors": ["article"]
    }
  ]
}
